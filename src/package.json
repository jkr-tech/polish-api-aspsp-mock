{
  "name": "polish-api-aspsp-mock",
  "version": "1.0.0",
  "description": "Mock implementation of ASPSP exposing Polish API interface.",
  "main": "target/index.js",
  "scripts": {
    "test": "nyc mocha test/**/*.{ts,tsx}",
    "build": "rm -rf target && tsc",
    "format": "tslint 'src/**/*.ts' --fix",
    "lint": "tslint 'src/**/*.ts'",
    "start": "node target",
    "generate-data": "node target/generator"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Grzegorz Abramczyk <grzegorz.abramczyk@tuatara.pl>",
  "license": "MIT",
  "devDependencies": {
    "@types/basic-auth": "^1.1.2",
    "@types/chai": "^4.1.7",
    "@types/debug": "0.0.31",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.117",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.1",
    "@types/passport": "^0.4.7",
    "@types/passport-local": "^1.0.33",
    "@types/session-file-store": "^1.2.0",
    "@types/swagger-tools": "^0.10.6",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.4"
  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "polish-api-generator": "git+ssh://git@github.com:tuatarapl/polish-api-generator.git",
    "polish-api-model": "git+ssh://git@github.com:tuatarapl/polish-api-model.git",
    "session-file-store": "^1.2.0",
    "swagger-tools": "^0.10.4",
    "uuid": "^3.3.2"
  }
}
